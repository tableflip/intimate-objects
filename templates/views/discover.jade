extends ../layouts/home
block content
  .container
    h2 Discover

    each obj, index in objects
      div.object
        a(href= '/discover/' + pad(4, obj.humanId+'', '0'))
          if obj.img.filename
            img.img-responsive(src="/images/sculptures/"+obj.img.filename)
          else
            span.no-image
          label.meta=pad(4, obj.humanId+'', '0')
        div.actions
